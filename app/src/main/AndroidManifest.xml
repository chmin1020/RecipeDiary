<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.FallTurtle.recipediary">

    <uses-permission android:name="android.permisson.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon1_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:requestLegacyExternalStorage="true"
        android:usesCleartextTraffic="true">

        <activity android:name="com.FallTurtle.recipediary.Activity.WebActivity"></activity>
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-7393868563420034~9579643485" />

        <activity android:name="com.FallTurtle.recipediary.Activity.ProgressDialogActivity" />
        <activity android:name="com.FallTurtle.recipediary.Activity.Google2Activity" />
        <activity android:name="com.FallTurtle.recipediary.Activity.GoogleActivity" />
        <activity android:name="com.FallTurtle.recipediary.Activity.InformationActivity" />
        <activity
            android:name="com.FallTurtle.recipediary.Activity.DataShoppingActivity"
            android:windowSoftInputMode="adjustNothing" />
        <activity
            android:name="com.FallTurtle.recipediary.Activity.AddShoppingActivity"
            android:windowSoftInputMode="adjustNothing" />
        <activity android:name="com.FallTurtle.recipediary.Activity.UpdateActivity" />
        <activity android:name="com.FallTurtle.recipediary.Activity.DataActivity" />
        <activity android:name="com.FallTurtle.recipediary.Activity.AddRecipeActivity" />
        <activity android:name="com.FallTurtle.recipediary.Activity.IntroActivity"></activity>
        <activity
            android:name="com.FallTurtle.recipediary.Activity.MainActivity"
            android:windowSoftInputMode="adjustNothing">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>